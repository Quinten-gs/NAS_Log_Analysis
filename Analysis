   "source": [
    "df = pd.read_csv('Data/syslog_2021-2-21-11 1 44.csv')\n",
    "df['Time'] = df.Time.apply(pd.Timestamp)\n",
    "df = df[df['Time'].apply(lambda l:l.year) >= 2021]\n",
    "df.index = df.Time\n",
    "gb = df.groupby(df.Time.apply(lambda l:l.hour)).count()\n",
    "px.line(gb,gb.index,'Log')"
   ]
